    "build": "concurrently \"babel src -d lib\" \"webpack --config config/webpack.prod.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yintingwen/vue-clipboard2.git"
  },
  "keywords": [
    "vue",
    "clipboard"
  ],
  "author": "sugar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yintingwen/vue-clipboard2/issues"
  },
  "homepage": "https://github.com/yintingwen/vue-clipboard2#readme",
  "module": "lib/index.js",
  "main": "dist/vue2-clipboard.min.js",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.14.7",
    "clipboard": "^2.0.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.0",
