{
  "name": "@ytw/vue2-clipboard",
  "version": "1.0.7",
  "description": "A clipboard plugin for vue2",
  "scripts": {
    "dev-lib": "concurrently \"babel src -d lib -w\" \"webpack serve --config config/webpack.dev.js\"",
    "dev-min": "concurrently \"babel src -d lib -w\" \"webpack --watch --config config/webpack.prod.js\"",
    "build": "npm run build-lib && npm run build-min",
    "build-lib": "rollup -c config/rollup.lib.js",
    "build-min": "rollup -c config/rollup.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yintingwen/vue-clipboard2.git"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "keywords": [
    "vue",
    "clipboard",
    "copy"
  ],
  "author": "sugar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yintingwen/vue-clipboard2/issues"
  },
  "homepage": "https://github.com/yintingwen/vue-clipboard2#readme",
  "module": "dist/vue2-clipboard.esm.js",
  "main": "dist/vue2-clipboard.min.js",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.14.7",
    "clipboard": "^2.0.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-node-resolve": "^13.0.1",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.0",
    "eslint": "^7.30.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "rollup": "^2.53.1",
    "rollup-plugin-terser": "^7.0.2",
    "vue": "^2.6.14"
  },
  "types": "./src/types/index.d.ts"
}
